{
  "compilerOptions": {
    "target": "ESNext",
    "types": ["cypress", "cypress-xpath"],
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "baseUrl": "cypress",
    "paths": {
      "pages/*": ["./cypress/page-objects/*"]
    }
  },
  "include": ["**/*.ts", "webpack.config.js"]
}
